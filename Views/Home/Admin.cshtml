
@{
    ViewBag.Title = "Admin";
}

<h2>Admin</h2>
<table border="1" class="table">
    <tr>
        <th>Name</th>
        <th>Description</th>
        <th>Quanity</th>
        <th>Price</th>
    </tr>

    @foreach (var item in ViewBag.List)
    {
        <tr>
            <td>@item.Name</td>
            <td>@item.Description</td>
            <td>@item.Quantity</td>
            <td>@item.Price</td>
            <td><a href="/Home/Itemdetails?itemName=@item.Name">Update</a></td>
            <td><a onclick="return confirm('Are you sure you want to delete')"></a></td>

        </tr>
    }



</table>



